                      <Column size="80"/>
                      <Column size="130"/>
                      <Column size="150"/>
                      <Column size="125"/>
                      <Column size="100"/>
                      <Column size="125"/>
                      <Column size="258"/>
                      <Column size="100"/>
                      <Column size="80"/>
                    </Columns>
                    <Rows>
                      <Row size="24" band="head"/>
                      <Row size="24"/>
                    </Rows>
                    <Band id="head">
                      <Cell text="ACCOUNT_CK" displaytype="checkboxcontrol" edittype="checkbox"/>
                      <Cell col="1" text="구분"/>
