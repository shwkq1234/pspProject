        </ColumnInfo>
        <Rows>
          <Row>
            <Col id="ACCOUNT_TYPE">매출</Col>
            <Col id="CODE">0</Col>
          </Row>
          <Row>
            <Col id="ACCOUNT_TYPE">매입</Col>
